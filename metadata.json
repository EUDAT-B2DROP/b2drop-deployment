{
  "name": "eudat-b2drop",
  "version": "0.0.2",
  "author": "eudat",
  "summary": "module for automated owncloud deployment with b2drop overlay",
  "license": "AGPL-3.0",
  "source": "https://github.com/EUDAT-B2DROP/b2drop-deployment.git",
  "project_page": "https://github.com/EUDAT-B2DROP/b2drop-deployment",
  "issues_url": "https://github.com/EUDAT-B2DROP/b2drop-deployment/issues",
